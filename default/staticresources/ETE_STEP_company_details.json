    "settings": {
        "data_object": {
            "Tax_ID__c": {
                "Hungary": {
                    "regex": "^\\d{8}-\\d{1}-\\d{2}$",
                    "maxlength": "13",
                    "mapto": "taxId",
                    "required": true
                },
                "Germany": {
                    "regex": "^((28|9|11|0|30|24|22|26|40|23|5|27|10|32|31|21|41|1|2){0,1}\\d{2,3}(0){0,1}\\d{3,4}\\d{3,4}\\d{1,1}$)",
                    "maxlength": "13",
                    "mapto": "taxId",
                    "required": false
                }
            },
            "Selected_Company_Name__c": {
                "Hungary": {
                    "regex": "",
                    "maxlength": "120",
                    "mapto": "selectedCompanyName",
                    "required": true
                },
                "Germany": {
                    "regex": "",
                    "maxlength": "120",
                    "mapto": "selectedCompanyName",
                    "required": true
                }
            },
            "Registered_Street__c": {
                "Hungary": {
                    "regex": "",
                    "maxlength": "255",
                    "mapto": "street",
                    "required": true
                },
                "Germany": {
                    "regex": "",
                    "maxlength": "255",
                    "mapto": "street",
                    "required": true
                }
            },
            "Registered_Postal_Code__c": {
                "Hungary": {
                    "regex": "",
                    "maxlength": "20",
                    "mapto": "postalCode",
                    "required": true
                },
                "Germany": {
                    "regex": "",
                    "maxlength": "20",
                    "mapto": "postalCode",
                    "required": true
                }
            },
            "Registered_City__c": {
                "Hungary": {
                    "regex": "",
                    "maxlength": "40",
                    "mapto": "city",
                    "required": true
                },
                "Germany": {
                    "regex": "",
                    "maxlength": "40",
                    "mapto": "city",
                    "required": true
                }
            },
            "Trade_Register_Number__c": {
                "Germany" : {
                    "regex": "^([Hh]{1}[Rr]{1}[AaBb]{1}[ ]{1}[0-9]{1,6}[ ]{0,1}[a-zA-Z]{0,5})+$",
                    "maxlength": "50",
                    "mapto": "tradeRegisterNumber",
                    "required": false
                }
            },
            "Vat_ID__c": {
                "Germany": {
                    "regex": "^([Dd]{1}[Ee]{1}[0-9]{9})+$",
                    "maxlength": "11",
