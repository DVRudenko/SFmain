<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CCS_Competition_Other</fullName>
    <active>true</active>
    <description>If CCS_Competition__c contains "Other", then CCS_Competition_Other__c is mandatory.</description>
    <errorConditionFormula>INCLUDES(CCS_Competition__c , "Other") &amp;&amp;   ISBLANK( CCS_Competition_Other__c )</errorConditionFormula>
    <errorDisplayField>CCS_Competition_Other__c</errorDisplayField>
    <errorMessage>Please enter a competitor.</errorMessage>
</ValidationRule>
